# AURORA BMI Environment Variables
# Copy this file to .env and fill in your API keys
#
# NOTE: Variable names match MoneyFlows 2026 convention

# Required API Keys
POLYGON_KEY=your_polygon_api_key_here
FMP_KEY=your_fmp_api_key_here

# Optional API Keys
UW_API_KEY=your_unusual_whales_api_key_here
FRED_KEY=your_fred_api_key_here

# Paths (optional, defaults shown)
DATA_DIR=data
CONFIG_DIR=config

# Logging
LOG_LEVEL=INFO
